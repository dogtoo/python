    var painter = new painterKline('kline',paintStream, getKLData());
    painterKline.prototype.preData = function() {
        //this.data.priceY = [];
        var kline = [];

        var fline = [];
        var tline = [];
        var sline = [];

        var priceYL = [];
        var dateXL = [];
        var priceY = {};
        var dateX = {};
        for (var i =ã€€140; i < 150; i++) {
            var rawData = data[i];
            priceY[Number(rawData[3])] = 1;
            priceY[Number(rawData[4])] = 1;
            dateX[rawData[0]] = 1;
            kline.push([rawData[0], rawData[2], rawData[5], rawData[3], rawData[4] ]);

            fline.push(MACount(5, i, data));
            tline.push(MACount(10, i, data));
            sline.push(MACount(20, i, data));
        }
        this.data.kline = kline;
        this.data.fline = fline;
        this.data.tline = tline;
        this.data.sline = sline;

        for (var pk in priceY)
            priceYL.push(pk);

        priceYL.sort(function(a, b) {
            return a - b;
        });
        this.data.priceY = priceYL;
        //Object.keys(priceY).reduce((a, c) => (a[c] = priceY[c], a), {});

        for (var dk in dateX)
            dateXL.push(dk);

        dateXL.sort(function(a, b) {
            return a - b;
        });
        this.data.dateX = dateXL;

        this.data.volumeY = ["0", "1000","2000"];
    }

    painterKline.prototype.start = function () {
        this.ctx.beginPath();
        this.ctx.strokeStyle = 'gray';
        this.ctx.save();
        this.ctx.translate(this.paintRegion['main.x']
                       , this.paintRegion['main.y']);

        this.ctx.rect(0, 0
                       , this.paintRegion['main.width']
                       , this.paintRegion['main.height']);
        this.ctx.stroke();
        var spaceHeight = 0;
        //console.log('height:'+this.paintRegion['price.height']+',length:'+(this.implements.priceY.options.horizontalLineCount - 1)+',fontHeight:'+this.implements.priceY.options.fontHeight / 2);
        /*
        for (var i = 1; i <= (this.implements.priceY.options.horizontalLineCount-2); i++) {
            spaceHeight = this.paintRegion['price.height'] * i / (this.implements.priceY.options.horizontalLineCount - 1) - (this.implements.priceY.options.fontHeight/2);
            var y = spaceHeight;
            this.drawHLine('#eeeeee', 0, y, this.paintRegion['main.width'], 1, 'solid');
        }*/

        for (var i in this.implements.priceY.options.y) {
            this.drawHLine('#eeeeee', 0, this.implements.priceY.options.y[i], this.paintRegion['main.width'], 1, 'solid');
        }
        /*
        console.log('lineWidth:'+this.implements.kline.options.lineWidth);
        console.log('width:'+this.paintRegion['main.width']+',Count:'+this.implements.dateX.options.verticalLineCount);
        var spaceWidth;
        for (var i = 1; i <= this.implements.dateX.options.verticalLineCount-2; i++) {
            spaceWidth = (this.paintRegion['main.width'] * i) / (this.implements.dateX.options.verticalLineCount - 1) - this.implements.kline.options.lineWidth / 2;
            var w = spaceWidth ;
            //if (w * 10 % 10 == 0) w += .5;
            this.drawVLine('#eeeeee', w, 0,this.paintRegion['main.height'], 1, 'solid');
        }*/
            var v_c = Math.round((this.implements.dateX.options.x.length)/this.implements.dateX.options.verticalLineCount);
            for (var i = 0 ; i < this.implements.dateX.options.x.length ; i+= v_c) {
                var diff = this.implements.dateX.options.x.length - i;
                if (diff < v_c) i = this.implements.dateX.options.x.length;
                this.drawVLine('#eeeeee', this.implements.dateX.options.x[i], 0,this.paintRegion['main.height'], 1, 'solid');
            }
        this.ctx.restore();
    }
}


