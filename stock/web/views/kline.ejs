<!DOCTYPE html>

<html lang="zh-Hant-TW">
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <script src="js/util.js" type="text/javascript" charset="UTF8"></script>
    <script src="js/absPainter.js" type="text/javascript" charset="UTF8"></script>
    <script src="js/axis-x.js" type="text/javascript" charset="UTF8"></script>
    <script src="js/axis-y.js" type="text/javascript" charset="UTF8"></script>
    <script src="js/k-data.js" type="text/javascript" charset="UTF8"></script>
</head>
<body>
    <h1 id="h1"><%=msg%></h1>
    <canvas id="kline" width="800" height="460" style="z-index: 2; border: 1px solid #69c"></canvas>
</body>
<script type="text/javascript">
    
    var painterKline = function(canvasId, paintStream, data) {
        Painter.call(this, canvasId, paintStream);
        this.data = data;
    };
    painterKline.prototype = Object.create(Painter.prototype);
    painterKline.prototype.constructor = painterKline;
    painterKline.prototype.preData = function() {
        //this.data = getKLData();
    }
    
    
    var paintStream = [
        {
            'price':{
                region: { x: 10, y: 10, width: 'f:00.00', height: '40%' },
                'priceY':new yAxis({
                    font: '11px Arial',
                    color: 'black',
                    align: 'right',
                    fontHeight: 8,
                    textBaseline: 'top',
                })
            }
        },
        {
            'main':{
                region: {
                    x: {'$and':['price.x', 'price.width']}, y: 0, width: {'$sub':['100%','price.width','padding.right']}, height: '40%',
                    backgroundColor:'#fff',
                    riseColor: 'red',
                    fallColor: 'green',
                    normalColor: 'black',
                    chartMargin:{left:45.5,top:20.5,right:20.5},
                    barWidth: 5, spaceWidth: 2, horizontalLineCount: 7, verticalLineCount: 7, lineStyle: 'solid', borderColor: 'gray', splitLineColor: '#eeeeee', lineWidth: 1
                },
                'kline':{

                },
                'dayline':{

                }
            }
        }/*,
        {
            'date':{
                region: { x: {'$and':['price.x', 'price.width']}, y: 'main.x', width: 'main.width', height: '5%' },
                'dateX':new xAxis({
                    font: '11px Arial',
                    color: 'black',
                    align: 'right',
                    fontHeight: 8,
                    textBaseline: 'top',
                    scalerCount: 9
                })
            }
        }*/
    ];
    var painter = new painterKline('kline',paintStream, getKLData());
    painterKline.prototype.preData = function() {
        this.data.priceY = [];
        var priceYL = [];
        var priceY = {};
        var dateX = {};
        for (var i = 0; i < this.data.ks.length; i++) {
            var rawData = data[i];
            //20111215,11.68,11.65,11.76,11.40,11.41,43356655,502325991
            priceY[Number(rawData[3])] = 1;
            priceY[Number(rawData[4])] = 1;
            dateX[rawData[0]] = 1;
        }
        
        for (var pk in priceY)
            priceYL.push(pk);
        
        priceYL.sort(function(a, b) {
            return a - b;
        });
        this.data.priceY = priceYL;
        //Object.keys(priceY).reduce((a, c) => (a[c] = priceY[c], a), {});
        
    }
    painter.init();
    painter.build();
</script>
</html>