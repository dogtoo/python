                    color: 'black',
                    align: 'right',
                    textBaseline: 'top',
                    dataType: 'money',
                    dataRange: 'range'
                })
            }
        },
        {
            'history':{
                region: { x: 'main.x', y: {'$and':['yHistory.y', determineFontHeight('11px Arial')/2]}, width: 'main.width', height: '12%' },
                'hline':new line({
                    yAxisName: 'yHistory',
                    strokeStyle: 'magenta',
                    lineWidth: 1
                }),
                'hisbar': cb
